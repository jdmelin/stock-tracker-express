{
  "name": "stock-tracker-express",
  "version": "1.0.0",
  "description": "Example app for DigitalCrafts Project 2",
  "main": "index.js",
  "scripts": {
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npm run db:drop; npm run db:create; npm run db:migrate; npm run db:seed; npm run dev",
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
